package proyectolistaligada;

public class Nodo {
    int dato;
    Nodo sig;
    Nodo prev;
    
    public Nodo(int d) {
        set_dato(d);
        set_sig(null);
        set_prev(null);
    }
    
    void set_dato(int d) {
        dato=d;
    }
    
    int get_dato() {
        return dato;
    }
    
    void set_sig(Nodo s) {
        sig=s;
    }
    
    Nodo get_sig() {
        return sig;
    }
    
    void set_prev(Nodo p) {
        prev=p;
    }
    
    Nodo get_prev() {
        return prev;
    }
}
